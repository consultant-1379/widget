<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'>

	<ui:with field='resourceBundle'
		type='com.ericsson.eniq.events.widgets.client.window.title.TitleWindowResourceBundle' />

	<g:SimplePanel styleName="{resourceBundle.style.outer}">
		<g:FlowPanel styleName="{resourceBundle.style.inner}">
			<g:HTMLPanel styleName="{resourceBundle.style.portlet}">
				<g:SimplePanel ui:field="headerContainer"></g:SimplePanel>
				<g:SimplePanel styleName="{resourceBundle.style.bodyContainer}"
					ui:field="bodyContainer" />
				<g:SimplePanel styleName="{resourceBundle.style.bodyContainer}"
					visible="false" ui:field="messageContainer" />
			</g:HTMLPanel>
		</g:FlowPanel>
	</g:SimplePanel>
</ui:UiBinder>